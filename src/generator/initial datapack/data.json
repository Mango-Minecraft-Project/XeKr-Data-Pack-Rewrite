{
  "const": {
    "lazy_name": "XKLazy",
    "lazy_namespace": "xklazy",
    "back_name": "XKBack",
    "back_namespace": "xkback",
    "smallmap_name": "XKSmallMap",
    "smallmap_namespace": "xksmallmap"
  },
  "content": {
    "pack.mcmeta": {
      "pack": {
        "pack_format": 12,
        "description": " crafting recipe data pack"
      },
      "filter": {}
    },
    "tag.json": {
      "replace": false,
      "values": []
    },
    "recipe.json": {}
  },
  "tree": [
    {
      "datapack": [
        {
          "%lazy_name%": [
            {
              "data": [
                {
                  "%lazy_namespace%": [
                    {
                      "recipes": [
                        "recipe.json"
                      ]
                    },
                    {
                      "tags": [
                        {
                          "items": [
                            "tag.json"
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            "pack.mcmeta"
          ]
        },
        {
          "%back_name%": [
            {
              "data": [
                {
                  "%back_namespace%": [
                    {
                      "recipes": [
                        "recipe.json"
                      ]
                    }
                  ]
                }
              ]
            },
            "pack.mcmeta"
          ]
        },
        {
          "%smallmap_name%": [
            {
              "data": [
                {
                  "%smallmap_namespace%": [
                    {
                      "recipes": [
                        "recipe.json"
                      ]
                    }
                  ]
                }
              ]
            },
            "pack.mcmeta"
          ]
        }
      ]
    }
  ]
}